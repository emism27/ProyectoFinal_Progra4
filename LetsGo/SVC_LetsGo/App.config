<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <connectionStrings>

    <!--Conexion BD Windows Autentication-->
    <!--Data Source = Nombre del Servidor de BD-->
    <add name="WIN_AUT"
         providerName="System.Data.SqlClient"
         connectionString="Data Source=localhost\I1;
                           Initial Catalog=BD_LETS_GO;
                           Integrated Security=True"/>

    <!--Conexion BD Sql Autentication-->
    <add name="SQL_AUT"
         providerName="System.Data.SqlClient"
         connectionString="Data Source=LAPTOP-HTU6GR2R;
                           Initial Catalog=BD_LETS_GO;
                           Persist Security info=False;
                           User ID=sa;
                           Password=sql"/>
  
  
  </connectionStrings>
  
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true"/>
  </appSettings>
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>
    <services>
        <service name="SVC_LetsGo.CONTRACT.LetsGo_Contract">
            <endpoint binding="basicHttpBinding" bindingConfiguration=""
                contract="SVC_LetsGo.INTERFACE.ILetsGo_Interface" />
            <endpoint address="mex" binding="mexHttpBinding" bindingConfiguration=""
                contract="IMetadataExchange" />
            <host>
                <baseAddresses>
                    <add baseAddress="http://localhost:8733/Design_Time_Addresses/SVC_LetsGo/CONTRACT/LetsGo_Contract" />
                </baseAddresses>
            </host>
        </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, 
          set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
          <!-- To receive exception details in faults for debugging purposes, 
          set the value below to true.  Set to false before deployment 
          to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="False"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup></configuration>
